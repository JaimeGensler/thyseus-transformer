{
	"name": "@thyseus/typescript-transformer",
	"version": "0.12.0-beta.7",
	"description": "Core of the official Thyseus transformer",
	"author": "Jaime Gensler",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/JaimeGensler/thyseus-transformer"
	},
	"keywords": [
		"thyseus",
		"typescript",
		"transformer",
		"transpiler",
		"compiler"
	],
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		"./src/*": "./src/*",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"dev": "pnpm test",
		"test": "vitest",
		"build": "vite build && tsup src/index.ts --dts-only"
	},
	"dependencies": {
		"typescript": "^5.0.4"
	},
	"devDependencies": {
		"thyseus": "^0.11.0",
		"tsup": "^6.7.0",
		"vite": "^4.2.1",
		"vitest": "^0.29.2"
	}
}
