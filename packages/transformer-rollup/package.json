{
	"name": "@thyseus/transformer-rollup",
	"version": "0.13.0",
	"description": "Rollup plugin for the official Thyseus transformer",
	"author": "Jaime Gensler",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/JaimeGensler/thyseus-transformer"
	},
	"keywords": [
		"thyseus",
		"typescript",
		"transformer",
		"transpiler",
		"compiler",
		"rollup"
	],
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		"./src/*": "./src/*",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"dev": "pnpm test",
		"test": "vitest",
		"build": "vite build && tsup src/index.ts --dts-only"
	},
	"peerDependencies": {
		"thyseus": "~0.13"
	},
	"devDependencies": {
		"rollup": "^3.21.8",
		"thyseus": "^0.11.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.6",
		"vitest": "^0.29.2"
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.0.2",
		"@thyseus/typescript-transformer": "workspace:0.12.0-beta.7"
	}
}
